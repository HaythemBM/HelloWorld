

{{> messages }}
{{#if username}}
<h2>Profil de {{ username }}</h2>
<table class="table">
  <tr>
    <td>Nom:</td>
    <td>{{ name }}</td>
  </tr>
  <tr>
    <td>GitHub:</td>
    <td>{{ github }}</td>
  </tr>
  <tr>
    <td>Twitter:</td>
    <td>{{ twitter }}</td>
  </tr>
  <tr>
    <td>Facebook:</td>
    <td>{{ facebook }}</td>
  </tr>
</table>
{{/if}}